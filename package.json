{
  "name": "Pet-N-Found",
  "version": "1.0.0",
  "description": "App to connect pet owners to their lost pets.",
  "main": "app.js",
  "scripts": {
    "test": "mocha",
    "dev": "npm-run-all -p sass:*",
    "setup": "node src/migrations/reset.js",
    "build-task:scss-compile": "node-sass --source-map true src/scss/ -o public/css",
    "build-task:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/scss/**/*.scss' -c 'npm run sass:build'",
    "migrate": "node src/migrations/migrate.js",
    "reset": "node src/migrations/reset.js",
    "seed": "node src/migrations/seed.js"
  },
  "author": "g",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "await-to-js": "^2.0.1",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "chokidar-cli": "^1.2.1",
    "config": "^2.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "leaflet-geosearch": "^2.7.0",
    "markdown": "^0.5.0",
    "mime": "^2.3.1",
    "multer": "^1.4.1",
    "node-gyp": "^3.8.0",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.1",
    "pug": "^2.0.3",
    "sequelize": "^4.39.1",
    "socket.io": "^2.1.1",
    "sqlite3": "^4.0.2",
    "uuid": "^3.3.2",
    "webfunc": "^0.14.0"
  },
  "repository": "https://github.com/Lokraan/Pet-N-Found",
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0"
  }
}
